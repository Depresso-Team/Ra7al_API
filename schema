        title: Username
        description: Required. 150 characters or fewer. Letters, digits and @/./+/-/_
          only.
        type: string
        pattern: ^[\w.@+-]+$
        maxLength: 150
        minLength: 1
      email:
        title: Email
        type: string
        format: email
        maxLength: 254
        minLength: 1
      password:
        title: Password
        type: string
        maxLength: 128
        minLength: 1
      phone:
        title: Phone
        type: integer
        maximum: 2147483647
        minimum: 0
        x-nullable: true
      address:
        title: Address
        type: string
        maxLength: 150
        minLength: 1
      country_code:
        title: Country code
        type: integer
        maximum: 2147483647
        minimum: 0
        x-nullable: true
      photo_url:
